{
  "name": "@pinecone-js/nest",
  "version": "0.1.16",
  "description": "PineconeJS â€” NestJS extensions that keep devs focused on business logic",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "@nestjs/common": ">=10.0.0",
    "@nestjs/core": ">=10.0.0",
    "zod": ">=4.0.0"
  },
  "devDependencies": {
    "tslib": ">=2.8.0",
    "tsup": "^8.5.0",
    "typescript": ">=5.9.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --target node18 --sourcemap --treeshake --out-dir dist --clean",
    "prepublishOnly": "npm run build && npm run test --if-present"
  },
  "keywords": [
    "nestjs",
    "framework",
    "architecture",
    "focus",
    "pineconejs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinecone-js/nest.git"
  },
  "bugs": {
    "url": "https://github.com/pinecone-js/nest/issues"
  },
  "homepage": "https://github.com/pinecone-js/nest"
}
